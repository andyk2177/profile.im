{
  "version": 2,
  "name": "profile-api",
  "build": {
    "maxLambdaSize": 50
  },
  "builds": [
    {
      "src": "dist/src/index.js",
      "use": "@now/node-server",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [{ "src": "/.*", "dest": "dist/src/index.js" }]
}
