{
  "version": 2,
  "name": "profile.im",
  "alias": "profile.im",
  "builds": [
    { "src": "www/package.json", "use": "@now/next" },
    { "src": "api/src/*.js", "use": "@now/node" }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/src/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/www/$1"
    }
  ]
}
